</table>
<table>
<name>vtiger_berlicleverreach_settings</name>
<sql><![CDATA[CREATE TABLE `vtiger_berlicleverreach_settings` (
  `id` int(11) NOT NULL,
  `customerid` varchar(16) COLLATE utf8_unicode_ci NOT NULL,
  `customername` varchar(128) COLLATE utf8_unicode_ci NOT NULL,
  `accesstoken` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  `newsubscribertype` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci]]></sql>
</table>
