<tables>
<table>
<name>vtiger_gdpr</name>
<sql><![CDATA[CREATE TABLE `vtiger_gdpr` (
  `gdprid` int(11) NOT NULL,
  `gdpr_no` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `gdprname` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL,
  `contactid` int(19) DEFAULT NULL,
  `leadid` int(19) DEFAULT NULL,
  `permission_check` int(1) NOT NULL DEFAULT '0',
  `permission_date` date DEFAULT NULL,
  `permission_basis` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `gdpr_comment` mediumtext COLLATE utf8_unicode_ci,
  `expiration_date` date DEFAULT NULL,
  PRIMARY KEY (`gdprid`),
  CONSTRAINT `fk_1_vtiger_gdpr` FOREIGN KEY (`gdprid`) REFERENCES `vtiger_crmentity` (`crmid`) ON DELETE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci]]></sql>
</table>
<table>
<name>vtiger_gdprcf</name>
<sql><![CDATA[CREATE TABLE `vtiger_gdprcf` (
  `gdprid` int(19) NOT NULL,
  PRIMARY KEY (`gdprid`),
  CONSTRAINT `fk_1_vtiger_gdprcf` FOREIGN KEY (`gdprid`) REFERENCES `vtiger_gdpr` (`gdprid`) ON DELETE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci]]></sql>
</table>
